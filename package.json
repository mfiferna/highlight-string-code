{
    "name": "highlight-string-code",
    "displayName": "Highlight String Code",
    "description": "Highlight string as SQL, HTML, CSS or JS in most languages. 高亮内嵌的SQL，HTML， CSS或者JS",
    "version": "0.4.5",
    "publisher": "mfiferna",
    "author": {
        "name": "mfiferna"
    },
    "icon": "docs/icon.png",
    "bugs": {
        "url": "https://github.com/mfiferna/highlight-string-code/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mfiferna/highlight-string-code"
    },
    "engines": {
        "vscode": "^1.6.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Other"
    ],
    "keywords": [
        "sql"
    ],
    "contributes": {
        "grammars": [
            {
                "path": "./syntaxes/highlight-string-code.json",
                "scopeName": "highlight-string-code",
                "injectTo": [
                    "text.html.basic",
                    "text.html.derivative",
                    "source",
                    "text"
                ],
                "embeddedLanguages": {
                    "meta.embedded.block.sql": "sql"
                },
                "tokenTypes": {
                    "meta.*": "other"
                }
            }
        ]
    }
}
